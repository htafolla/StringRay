{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "preemptive_compaction": true,
  "plugins": ["stringray-framework"],
  "model_routing": {
    "enforcer": "opencode/grok-code",
    "architect": "opencode/grok-code",
    "orchestrator": "opencode/grok-code",
    "bug-triage-specialist": "opencode/grok-code",
    "code-reviewer": "opencode/grok-code",
    "security-auditor": "opencode/grok-code",
    "refactorer": "opencode/grok-code",
    "test-architect": "opencode/grok-code",
    "sisyphus": "opencode/grok-code",
    "librarian": "opencode/grok-code",
    "explore": "opencode/grok-code",
    "oracle": "opencode/grok-code",
    "multimodal-looker": "opencode/grok-code"
  },
  "logging": {
    "enabled": true,
    "refactoring_log_path": ".opencode/REFACTORING_LOG.md",
    "auto_capture": true,
    "format": "markdown",
    "include_timestamps": true,
    "categories": ["refactoring", "analysis", "performance", "security"]
  },
  "agents": {
    "librarian": {
      "model": "opencode/grok-code",
      "description": "Codebase and documentation search specialist",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
          "read": true,
          "grep": true,
          "run_terminal_cmd": true
      },
      "permission": {
        "edit": "deny",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "explore": {
      "model": "opencode/grok-code",
      "description": "Fast codebase exploration and pattern analysis specialist",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
          "read": true,
          "grep": true,
          "run_terminal_cmd": true
      },
      "permission": {
        "edit": "deny",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "frontend-ui-ux-engineer": {
      "model": "opencode/grok-code",
      "description": "Frontend development and UI/UX implementation specialist",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
          "read": true,
          "grep": true,
          "run_terminal_cmd": true
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "document-writer": {
      "model": "opencode/grok-code",
      "description": "Technical documentation and content creation specialist",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
          "read": true,
          "write": true,
          "run_terminal_cmd": true
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "multimodal-looker": {
      "model": "opencode/grok-code",
      "description": "Multimodal file analysis and interpretation specialist for images, diagrams, PDFs, and media files",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
          "read": true,
          "run_terminal_cmd": true,
          "look_at": true
      },
      "permission": {
        "edit": "deny",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "oracle": {
      "model": "opencode/grok-code",
      "description": "Strategic guidance and complex problem-solving specialist for architectural decisions",
      "mode": "subagent",
      "temperature": 0.3,
      "tools": {
          "read": true,
          "grep": true,
          "run_terminal_cmd": true
