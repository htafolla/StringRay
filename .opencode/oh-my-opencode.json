        }
      }
    },
    "Sisyphus": {
      "model": "opencode/grok-code",
      "description": "Main orchestrator and command center for multi-agent coordination and workflow management",
      "mode": "primary",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "run_terminal_cmd",
          "background_task",
          "call_omo_agent",
          "session_*",
          "task",
          "todowrite",
          "todoread",
          "skill"
        ]
      },
      "permission": {
