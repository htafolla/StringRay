{
  "name": "strray-ai",
  "version": "1.1.0",
  "description": "⚡ StringRay ⚡: Bulletproof AI orchestration with systematic error prevention. Zero dead ends. Ship clean, tested, optimized code — every time.",
  "type": "module",
  "main": "./dist/plugin/plugins/stringray-codex-injection.js",
  "types": "./dist/plugin/plugins/stringray-codex-injection.d.ts",
  "bin": {
    "strray-ai": "./dist/cli/index.js"
  },
    "oh-my-opencode": {
      "plugin": "./dist/plugin/plugins/stringray-codex-injection.js",
      "mcps": "./dist/plugin/mcps/"
    },

  "scripts": {
    "build": "tsc",
    "build:all": "npm run build && npm run build:plugin",
    "build:plugin": "tsc --project tsconfig.plugin.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build:all",
    "postinstall": "node scripts/postinstall.cjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test:unit": "vitest run",
    "test:integration": "node scripts/test-consumer-readiness.mjs && node scripts/test-mcp-functionality.mjs && node scripts/test-postinstall-files.mjs",
    "test:e2e": "node scripts/validation/validate-mcp-connectivity.js && node scripts/validation/validate-oh-my-opencode-integration.js && node scripts/validation/validate-external-processes.js",
    "test:comprehensive": "./scripts/validate-stringray-framework.sh",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:comprehensive",
    "test:coverage": "vitest run --coverage",
    "test:security-audit": "node scripts/validation/validate-mcp-connectivity.js",
    "test:dependency-scan": "node scripts/test-consumer-readiness.mjs",
    "ci-install": "npm ci --ignore-scripts"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "codex",
    "development",
    "automation",
    "typescript",
    "framework"
  ],
  "author": "StrRay Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htafolla/stringray.git"
  },
  "bugs": {
    "url": "https://github.com/htafolla/stringray/issues"
  },
  "homepage": "https://stringray.dev",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "oh-my-opencode": "^2.12.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@opencode-ai/sdk": "^1.0.0",
    "commander": "^12.0.0",
    "structlog": "^1.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^20.0.0",
    "@types/yaml": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "jsdom": "^25.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^2.0.0"
  },
  "files": [
    ".mcp.json",
    "opencode.json",
    ".opencode/",
    ".strray/",
    "dist/plugin/",
    "scripts/postinstall.cjs"
  ],
  "exclude": [
    ".opencode/state"
  ],
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    },
    "./dist/plugin/orchestrator.js": "./dist/plugin/orchestrator.js",
    "./dist/plugin/processors/processor-manager.js": "./dist/plugin/processors/processor-manager.js",
    "./dist/plugin/state/state-manager.js": "./dist/plugin/state/state-manager.js",
    "./dist/plugin/agents/index.js": "./dist/plugin/agents/index.js",
    "./dist/plugin/*": "./dist/plugin/*",
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator.d.ts",
      "import": "./dist/orchestrator.js"
    },
    "./codex": {
      "types": "./dist/codex-injector.d.ts",
      "import": "./dist/codex-injector.js"
    },
    "./memory-monitor": {
      "types": "./dist/monitoring/memory-monitor.d.ts",
      "import": "./dist/monitoring/memory-monitor.js"
    },
    "./memory-pools": {
      "types": "./dist/utils/memory-pool.d.ts",
      "import": "./dist/utils/memory-pool.js"
    },
    "./memory-regression": {
      "types": "./dist/testing/memory-regression-suite.d.ts",
      "import": "./dist/testing/memory-regression-suite.js"
    }
  }
}
