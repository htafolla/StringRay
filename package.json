{
  "name": "strray",
  "version": "1.0.0",
  "description": "Bulletproof AI orchestration with systematic error prevention. Zero dead ends. Ship clean, tested, optimized code â€” every time.",
  "type": "module",
  "main": "./dist/plugin/index.js",
  "types": "./dist/plugin/index.d.ts",
  "oh-my-opencode": {
    "plugin": "./dist/plugin/plugins/strray-codex-injection.js"
  },
  "bin": {
    "strray": "scripts/setup.cjs"
  },
  "scripts": {
    "build": "tsc",
    "build:plugin": "tsc --project tsconfig.plugin.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build:plugin",
    "postinstall": "node scripts/postinstall.cjs",
    "ci-monitor": "node scripts/github-actions-monitor.cjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "framework:init": "bash .opencode/init.sh",
    "validate": "npm run build && npm run typecheck",
    "ci-report": "echo 'CI/CD health check passed'",
    "test:performance": "vitest run src/__tests__/performance/",
    "performance:report": "node scripts/performance-report.js",
    "performance:gates": "node src/performance/performance-ci-gates.js",
    "test:architect": "vitest run src/__tests__/agents/architect.test.ts src/__tests__/agents/test-architect.test.ts",
    "test:e2e": "vitest run src/__tests__/integration/e2e-framework-integration.test.ts",
    "test:path-resolution": "node scripts/test-comprehensive-path-resolution.mjs",
    "test:mcp-integration": "opencode mcp list && echo 'MCP servers verified'",
    "test:all": "npm run test && npm run test:architect && npm run test:e2e && npm run test:path-resolution",
    "security-audit": "npm audit --audit-level moderate --production"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "codex",
    "development",
    "automation",
    "typescript",
    "framework"
  ],
  "author": "StrRay Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htafolla/strray.git"
  },
  "bugs": {
    "url": "https://github.com/htafolla/strray/issues"
  },
  "homepage": "https://strray.dev",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "oh-my-opencode": "^2.12.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@opencode-ai/sdk": "^1.0.0",
    "structlog": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/yaml": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^22.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist/plugin",
    "dist/mcps",
    "dist/agents",
    "dist/orchestrator",
    "dist/processors",
    "dist/state",
    "dist/utils",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator.d.ts",
      "import": "./dist/orchestrator.js"
    },
    "./codex": {
      "types": "./dist/codex-injector.d.ts",
      "import": "./dist/codex-injector.js"
    },
    "./memory-monitor": {
      "types": "./dist/monitoring/memory-monitor.d.ts",
      "import": "./dist/monitoring/memory-monitor.js"
    },
    "./memory-pools": {
      "types": "./dist/utils/memory-pool.d.ts",
      "import": "./dist/utils/memory-pool.js"
    },
    "./memory-regression": {
      "types": "./dist/testing/memory-regression-suite.d.ts",
      "import": "./dist/testing/memory-regression-suite.js"
    }
  }
}
