{
	"name": "stringray-framework",
	"version": "1.0.0",
	"description": "StringRay (StrRay) - Ship Production-Ready Code & Eliminate Common Dead Ends for AI Devs. Eradicates Spaghetti Code, Hallucinations & Code Rot with Agentic AI Coordination & Automation.",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc && node dist/server.js",
		"start": "node dist/server.js",
		"stop": "pkill -f 'node dist/server.js'",
		"init": "bash scripts/init.sh",
		"validate": "node scripts/validate-codex.js",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run build",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage --reporter=verbose",
		"test:unit": "vitest run --config vitest.unit.config.ts",
		"test:performance": "node scripts/performance-test.cjs",
		"audit": "npm audit",
		"security-audit": "node scripts/basic-security-audit.cjs",
		"security-check": "npm run security-audit && npm audit --audit-level=moderate",
		"security-monitor": "node scripts/basic-security-audit.cjs --monitor",
		"deps:check": "npm audit && npm outdated",
		"deps:update": "npm update && npm audit fix",
		"deps:scan": "node scripts/dependency-scan.cjs"
	},
	"keywords": [
		"ai",
		"development",
		"framework",
		"typescript",
		"codex",
		"error-prevention",
		"oh-my-opencode"
	],
	"author": "StrRay Framework Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/strray-framework/stringray.git"
	},
	"homepage": "https://github.com/strray-framework/stringray#readme",
	"bugs": {
		"url": "https://github.com/strray-framework/stringray/issues"
	},
	"dependencies": {
		"@opencode-ai/plugin": "~1.1.4",
		"@types/express": "~5.0.6",
		"express": "~5.2.1",
		"open": "~11.0.0",
		"zod": "~4.1.8"
	},
	"devDependencies": {
		"@types/node": "~22.19.3",
		"@vitest/coverage-v8": "~4.0.16",
		"typescript": "~5.9.3",
		"vitest": "~4.0.16"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"files": [
		"dist",
		"src",
		"scripts",
		"config",
		"docs",
		"README.md"
	]
}
