{
  "name": "strray-ai",
  "version": "1.0.6",
  "description": "âš¡ StringRay âš¡: Bulletproof AI orchestration with systematic error prevention. Zero dead ends. Ship clean, tested, optimized code â€” every time.",
  "type": "module",
  "main": "./dist/plugin/index.js",
  "types": "./dist/plugin/index.d.ts",
  "oh-my-opencode": {
    "plugin": "./dist/plugin/plugins/stringray-codex-injection.js"
  },
  "bin": {
    "strray-ai": "dist/cli/index.js",
    "strray-test": "scripts/test-stringray-plugin.mjs"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build && npm run build:plugin",
    "build:plugin": "tsc --project tsconfig.plugin.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build:plugin",
    "postinstall": "node scripts/postinstall.cjs",
    "ci-install": "npm ci --ignore-scripts && npm run postinstall",
    "preinstall": "echo 'ðŸ”§ Installing StringRay Framework dependencies...'",
    "ci-monitor": "node scripts/github-actions-monitor.cjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "framework:init": "bash .opencode/init.sh",
    "validate": "npm run lint && npm run typecheck && npm run test:all && npm run build",
    "ci-report": "echo 'CI/CD health check passed'",
    "test:performance": "vitest run src/__tests__/performance/",
    "performance:report": "node scripts/performance-report.js",
    "performance:gates": "node scripts/run-performance-gates.mjs",
    "test:architect": "vitest run src/__tests__/agents/architect.test.ts src/__tests__/agents/test-architect.test.ts",
    "test:e2e": "vitest run src/__tests__/integration/e2e-framework-integration.test.ts",
    "test:path-resolution": "node scripts/test-comprehensive-path-resolution.mjs",
    "test:plugin": "node scripts/test-stringray-plugin.mjs",
    "test:orchestration": "vitest run src/__tests__/unit/orchestrator.test.ts src/__tests__/integration/orchestrator/",
    "test:mcp-integration": "opencode mcp list && echo 'MCP servers verified'",
    "test:mcp-connectivity": "node scripts/validation/validate-mcp-connectivity.js",
    "test:oh-my-opencode-integration": "node scripts/validation/validate-oh-my-opencode-integration.js",
    "test:external-processes": "node scripts/validation/validate-external-processes.js",
    "test:comprehensive": "npm run test:mcp-connectivity && npm run test:oh-my-opencode-integration && npm run test:external-processes",
    "test:all": "npm run test && npm run test:architect && npm run test:e2e && npm run test:path-resolution && npm run test:comprehensive",
    "security-audit": "npm audit --audit-level moderate --production",
    "test:ci": "CI=true npm test -- --run --reporter=json --no-coverage",
    "test:coverage": "vitest run --coverage",
    "test:npm-install": "./scripts/test-npm-install.sh",
    "test:simple-npm": "./scripts/test-simple-npm.sh",
    "test:security-audit": "node scripts/basic-security-audit.cjs",
    "test:dependency-scan": "node scripts/dependency-scan.cjs",
    "test:unit": "vitest run src/__tests__/unit/",
    "test:integration": "vitest run src/__tests__/integration/",
    "test:e2e-complete": "npm run test:e2e && node scripts/test-enforcement-e2e.mjs",
    "test:simulation": "node scripts/run-simulations.mjs",
    "test:monitoring": "node scripts/monitoring/monitoring-daemon.mjs",
    "test:validation": "npm run test:mcp-connectivity && npm run test:oh-my-opencode-integration && node scripts/validate-codex.js",
    "ci-fix": "node scripts/ci-cd-auto-fix.cjs"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "codex",
    "development",
    "automation",
    "typescript",
    "framework"
  ],
  "author": "StrRay Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htafolla/stringray.git"
  },
  "bugs": {
    "url": "https://github.com/htafolla/stringray/issues"
  },
  "homepage": "https://stringray.dev",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "oh-my-opencode": "^2.12.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@opencode-ai/sdk": "^1.0.0",
    "structlog": "^1.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/yaml": "^1.9.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "jsdom": "^25.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^2.0.0"
  },
  "files": [
    ".mcp.json",
    "opencode.json",
    ".opencode/oh-my-opencode.json",
    ".opencode/package.json",
    ".claude/.mcp.json",
    "scripts/postinstall.cjs",
    "scripts/validation",
    "dist/cli",
    "dist/plugin",
    "dist/mcps",
    "dist/agents",
    "dist/orchestrator",
    "dist/processors",
    "dist/state",
    "dist/utils",
    "scripts/postinstall.cjs",
    "scripts/test-stringray-plugin.mjs",
    "scripts/test-path-resolver.mjs",
    "scripts/validate-codex.js",
    "scripts/test-comprehensive-path-resolution.mjs",
    "scripts/test-full-plugin-no-timeout.sh",
    "scripts/test-simple-prompt.mjs",
    "scripts/test-complex-orchestration.mjs",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator.d.ts",
      "import": "./dist/orchestrator.js"
    },
    "./codex": {
      "types": "./dist/codex-injector.d.ts",
      "import": "./dist/codex-injector.js"
    },
    "./memory-monitor": {
      "types": "./dist/monitoring/memory-monitor.d.ts",
      "import": "./dist/monitoring/memory-monitor.js"
    },
    "./memory-pools": {
      "types": "./dist/utils/memory-pool.d.ts",
      "import": "./dist/utils/memory-pool.js"
    },
    "./memory-regression": {
      "types": "./dist/testing/memory-regression-suite.d.ts",
      "import": "./dist/testing/memory-regression-suite.js"
    }
  }
}
