{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "preemptive_compaction": true,

  "plugins": ["stringray-framework"],
  "model_routing": {
    "enforcer": "opencode/grok-code",
    "architect": "opencode/grok-code",
    "orchestrator": "opencode/grok-code",
    "bug-triage-specialist": "opencode/grok-code",
    "code-reviewer": "opencode/grok-code",
    "security-auditor": "opencode/grok-code",
    "refactorer": "opencode/grok-code",
    "test-architect": "opencode/grok-code",
    "sisyphus": "opencode/grok-code"
  },
  "logging": {
    "enabled": true,
    "refactoring_log_path": ".opencode/REFACTORING_LOG.md",
    "auto_capture": true,
    "format": "markdown",
    "include_timestamps": true,
    "categories": ["refactoring", "analysis", "performance", "security"]
  },
  "agents": {
    "librarian": {
      "model": "opencode/grok-code"
    },
    "explore": {
      "model": "opencode/grok-code"
    },
    "frontend-ui-ux-engineer": {
      "model": "opencode/grok-code"
    },
    "document-writer": {
      "model": "opencode/grok-code"
    },
    "multimodal-looker": {
      "model": "opencode/grok-code"
    },
    "enforcer": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework enforcer with error handling and performance facilities",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": ["read", "grep", "lsp_*", "run_terminal_cmd"]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "architect": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework architect with state management and delegation",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "background_task"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "orchestrator": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework orchestrator with coordination and workflow management",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "background_task",
          "call_omo_agent"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "bug-triage-specialist": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework bug triage specialist with error investigation and surgical fixes",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "ast_grep_search",
          "ast_grep_replace"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "code-reviewer": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework code reviewer with monitoring and analytics capabilities",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "lsp_diagnostics",
          "lsp_code_actions"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "security-auditor": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework security auditor with compliance and vulnerability detection",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "grep_app_searchGitHub"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "refactorer": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework refactorer with technical debt elimination and code consolidation",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "ast_grep_search",
          "ast_grep_replace",
          "lsp_rename"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow"
        }
      }
    },
    "test-architect": {
      "model": "opencode/grok-code",
      "description": "StrRay Framework test architect with automatic test generation, coverage optimization, and behavioral testing",
      "mode": "subagent",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "run_terminal_cmd",
          "write"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow",
          "test": "allow"
        }
      }
    },
    "sisyphus": {
      "model": "opencode/grok-code",
      "description": "Main orchestrator and command center for multi-agent coordination and workflow management",
      "mode": "primary",
      "temperature": 0.1,
      "tools": {
        "include": [
          "read",
          "grep",
          "lsp_*",
          "run_terminal_cmd",
          "run_terminal_cmd",
          "write",
          "background_task",
          "call_omo_agent",
          "session_*",
          "task",
          "todowrite",
          "todoread",
          "webfetch",
          "websearch",
          "codesearch",
          "skill",
          "webfetch",
          "bash",
          "read",
          "glob",
          "grep",
          "edit",
          "write",
          "task",
          "todowrite",
          "todoread",
          "webfetch",
          "websearch",
          "codesearch",
          "skill",
          "lsp_hover",
          "lsp_goto_definition",
          "lsp_find_references",
          "lsp_document_symbols",
          "lsp_workspace_symbols",
          "lsp_diagnostics",
          "lsp_servers",
          "lsp_prepare_rename",
          "lsp_rename",
          "lsp_code_actions",
          "lsp_code_action_resolve",
          "ast_grep_search",
          "ast_grep_replace",
          "session_list",
          "session_read",
          "session_search",
          "session_info",
          "background_task",
          "background_output",
          "background_cancel",
          "call_omo_agent",
          "background_output",
          "look_at",
          "skill_mcp",
          "slashcommand",
          "interactive_bash",
          "skill_mcp",
          "slashcommand",
          "interactive_bash",
          "look_at"
        ]
      },
      "permission": {
        "edit": "allow",
        "bash": {
          "git": "allow",
          "npm": "allow",
          "bun": "allow",
          "test": "allow",
          "coverage": "allow"
        }
      }
    }
  }
}
